{
  "manifest_version": 2,
  "name": "G-web",
  "version": "1.8",
  "description": "更改新标签页为个人定制页面，还有一个可爱的私人猫猫（可通过 Alt+C 开启/关闭），音乐组件可以检测到浏览器目前所播放的音乐或者视频，左侧有快捷栏。",

  "icons": {
    "16": "icon1.png",
    "48": "icon1.png",
    "128": "icon1.png"
  },

  "chrome_url_overrides": {
    "newtab": "index.html"
  },

  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "https://suggestion.baidu.com/*",
    "<all_urls>"
  ],

  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },

  "web_accessible_resources": [
    "main.js",
    "index.html",
    "video3.mp4",
    "poster.jpg",
    "icon1.png",
    "bing-logo.png",
    "google-logo.png",
    "baidu-logo.png",
    "sogou-logo.png",
    "bing-logo-small.png",
    "google-logo-small.png",
    "baidu-logo-small.png",
    "sogou-logo-small.png",
    "2025102523114053.png",
    "2025102514322810.png",
    "小猫/*.webm",
    "小猫/*.png",
    "page-media.js",
    "content-media.js"
  ],

  "content_security_policy": "script-src 'self'; object-src 'self'"
}
